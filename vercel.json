{
    "version": 2,
    "routes": [
      {
        "src": "/[^.]+",
        "dest": "/",
        "status": 200
      },
      {
        "src": "/static/(.*)",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        }
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ]
  }